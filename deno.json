{
  "tasks": {
    "build-mac-intel": "deno compile --allow-env --allow-run --target x86_64-apple-darwin --output bin/branch-cli-mac-intel src/main.ts",
    "build-mac": "deno compile --allow-env --allow-run --target aarch64-apple-darwin --output bin/branch-cli-mac src/main.ts",
    "build-windows": "deno compile --allow-env --allow-run --target x86_64-pc-windows-msvc --output bin/branch-cli.exe src/main.ts",
    "build": "deno task build-mac-intel && deno task build-mac && deno task build-windows",
    "chmod": "chmod +x bin/*"
  },
  "imports": {
    "@inquirer/select": "npm:@inquirer/select@^4.0.9",
    "@sallai/ask": "jsr:@sallai/ask@^2.0.2",
    "@sallai/iro": "jsr:@sallai/iro@^1.0.4"
  }
}
